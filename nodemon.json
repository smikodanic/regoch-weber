{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/*",
    "client/cache",
    "client/_dist"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "server",
    "sys/server"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
